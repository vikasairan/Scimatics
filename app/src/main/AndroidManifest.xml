<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.scimatics.Scimatics">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lead"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/scimatics"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Aboutus" />
        <activity android:name=".event" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".contact" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".MainLoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".AddUser" />
        <activity android:name=".DeleteUser" />
        <activity android:name=".UpdateUser" />
        <activity
            android:name=".UserActivity"
            android:launchMode="singleInstance" />
        <activity android:name=".UploadImage" />
        <activity android:name=".Edit" />
        <activity android:name=".Feedback" />
        <activity
            android:name=".MemberDetails"
            android:launchMode="singleInstance" />
        <activity
            android:name=".Member_Information"
            android:launchMode="singleInstance" />
        <activity android:name=".list" />
        <activity android:name=".Latest_Updates"
            android:launchMode="singleInstance"/>
    </application>

</manifest>